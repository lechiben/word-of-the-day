{
  "manifest_version": 3,
  "name": "Word of the Day - GRE Vocabulary",
  "version": "1.1",
  "description": "Learn a new GRE vocabulary word every time you open a new tab",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://raw.githubusercontent.com/*"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}