{
  "manifest_version": 3,
  "name": "Word of the Day",
  "version": "1.0",
  "description": "Learn a new vocabulary word every time you open a new tab",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
